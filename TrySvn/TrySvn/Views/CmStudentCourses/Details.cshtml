@model TrySvn.Models.CmStudentCourse

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>CmStudentCourse</legend>

    <div class="display-label">cmStudent</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.cmStudent).ToString()</div>

    <div class="display-label">clSubject</div>
    <div class="display-field">@(Model.clSubject == null ? "None" : Model.clSubject.Name)</div>

    <div class="display-label">cmTeacher</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.cmTeacher).ToString()</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>


