@model IEnumerable<TrySvn.Models.CmStudent>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            yearStudy
        </th>
        <th>
            course
        </th>
        <th>
            cmUser
        </th>
        <th>
            clUniversity
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
        <td>
			@item.yearStudy
        </td>
        <td>
			@item.course
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.cmUser).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.clUniversity).ToString()
        </td>
    </tr>
}

</table>


